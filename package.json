    "@nestjs/swagger": "^4.5.9",
    "@nestjs/websockets": "^7.1.3",
    "@types/oracledb": "^4.2.1",
    "@types/websocket": "^1.0.0",
    "@types/ws": "^7.2.5",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^9.0.0",
    "nest-morgan": "^1.0.1",
    "npm-check-updates": "^6.0.1",
    "oracledb": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "socket.io-redis": "^5.3.0",
